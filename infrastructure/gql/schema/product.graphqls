type Product {
    id: ID
    NamaProduct: String
    JenisProduct: String
    HargaProduct: Float
}

input ProductRequest {
    id: ID!
    NamaProduct: String!
    JenisProduct: String!
    HargaProduct: Float!
}

type ProductResponse {
    StatusCode: String!
    StatusDesc: String!
    Data: Product
    DataList: [Product]
}

extend type Mutation {
    CreateProduct(request: ProductRequest!):ProductResponse
}

extend type Query {
    GetAllProduct(): ProductResponse
    GetProductById(id: ID!): ProductResponse
}